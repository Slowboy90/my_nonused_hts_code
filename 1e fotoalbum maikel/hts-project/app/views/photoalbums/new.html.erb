<% provide(:title, "New album") %>
<h2> Wat is de naam van het nieuwe album? </h2>
<%= form_for @photoalbum do |f| %>
	<div class='form-group'>
		<%= f.text_field :name, class: 'form-control' %>
	</div>
	<div class='form-group' >
		<%= f.submit 'Verstuur', class: 'btn btn-primary' %>
	</div>
<% end %>
